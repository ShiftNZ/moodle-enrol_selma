<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="enrol/selma/db" VERSION="20200619" COMMENT="XMLDB file for Moodle enrol/selma"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="enrol_selma_log" COMMENT="Application log for plugin.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="message" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="level" TYPE="int" LENGTH="3" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="levelname" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="channel" TYPE="char" LENGTH="64" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="context" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timestamp" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="level" UNIQUE="false" FIELDS="level"/>
        <INDEX NAME="timestamp" UNIQUE="true" FIELDS="timestamp"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>